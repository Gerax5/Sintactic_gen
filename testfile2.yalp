/* Parser simple que reconoce asignaciones y expresiones aritm√©ticas */

%token IDENTIFICADOR
%token NUMBER
%token PLUS
%token MINUS
%token ASSIGN

%%

operacion:
    IDENTIFICADOR ASSIGN expression
;

expression:
    expression PLUS term
  | expression MINUS term
  | term
;

term:
    NUMBER
  | IDENTIFICADOR
;
